        <label xml:lang="it">Volt Partenza</label>
        <label xml:lang="fr">V démarr.</label>
        <label xml:lang="de">Startspannung</label>
        <label xml:lang="nl">Startspanning</label>
        <comment>Sets the output voltage to the motor at speed level 1</comment>
      </variable>
      <variable CV="3" comment="Sets the acceleration rate (delay)" item="Accel" default="16">
        <decVal max="255" />
        <label>Acceleration Rate</label>
        <label xml:lang="it">Accellerazione (0-255)</label>
        <label xml:lang="fr">Accelération (0-255)</label>
        <label xml:lang="de">Anfahr0verzögerung (0-255)</label>
        <label xml:lang="nl">Optrekconstante (0-255)</label>
        <comment>Sets the acceleration rate (delay)</comment>
