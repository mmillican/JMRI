				<choice xml:lang="de">Bank  0 - CV31 (Keine Bank)</choice>
				<choice xml:lang="nl">CV31 = 0</choice>
				<choice xml:lang="cs">CV31 = 0</choice>
			</enumChoice>
			<!-- not maintained 
			<enumChoice choice="Bank 15" value="15">
				<choice>Bank 15 - Hardware</choice>
				<choice xml:lang="de">Bank 15 - CV31</choice>
				<choice xml:lang="nl">Bank 15 - CV31</choice>
